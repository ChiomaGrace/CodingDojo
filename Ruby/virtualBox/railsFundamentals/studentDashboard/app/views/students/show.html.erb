<div class="container">
    <h1> Seattle's <%= @specificStudent.firstName %></h1>
    <p><%= @specificStudent.email %></p>
    <h2>Cohort</h2>
    <table class="table table-hover">
    <thead>
        <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        </tr>
        <% @studentsInDojo.each do |student| %>
        <tr>
        <td><%= student.firstName %></td>
        <td><%= student.lastName %></td>
        <td><%= student.email %></td>
        </tr>
        <% end %>
    </thead>
    <tbody>

    </tbody>
    </table>
    <a href="/dojos">Home</a>
    <a href="/dojos/<%= @specificDojo.id %>/students/<%= @specificStudent.id %>/edit">Edit</a>

</div>


