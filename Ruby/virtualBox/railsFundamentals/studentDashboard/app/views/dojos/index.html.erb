<div class="container">
    <h1>Listing 4 Dojos</h1>
    <table class="table table-hover">
    <thead>
        <tr>
        <th scope="col">Coding Dojo Branch</th>
        <th scope="col">Street</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
        <th scope="col">Actions</th>

        </tr>
    </thead>
    <tbody>
        <tr>
        <%@dojos.each do |dojo| %>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td>
                <a href="/dojos/<%= dojo.id %>">Show</a>
                <a href="">Edit</a>
                <form action="/dojos/<%= dojo.id %>/destroy"method="post">
                    <input type="hidden" name="_method" value="delete">
                    <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
                    <button type="submit" class="btn btn-link" value="Destroy">Destroy</button>
                </form>
        </td>
        </tr>
        <% end %>
    </tbody>
    </table>
    <a href="dojos/new">New Dojo</a>
</div>
