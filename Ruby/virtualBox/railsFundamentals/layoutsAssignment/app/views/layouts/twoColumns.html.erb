<!DOCTYPE html>
<html>
<head>
    <title>LayoutsAssignment</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <%= yield :leftColumn %>
                <%# One of two columns %>
            </div>
            <div class="col-sm-6">
                <%= yield :rightColumn %>
                <%# two of two columns %>
            </div>
        </div>
    </div>

<%= yield %>
    
</body>
</html>