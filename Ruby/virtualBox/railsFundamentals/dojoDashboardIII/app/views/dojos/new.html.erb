<h1>New Dojo</h1>

<% if @errors%>
    <% @errors.each do |error| %>
    <p class = 'danger'><%= error %></p>
    <% end %>
<% end%>

<form action="/dojos" method="post">
    <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
    <p><input type="text" name='dojo[branch]' placeholder="branch"></p>
    <p><input type="text" name='dojo[street]' placeholder="street"></p>
    <p><input type="text" name='dojo[city]' placeholder="city"></p>
    <p><input type="text" name='dojo[state]' placeholder="state"></p>
    <input type="submit" value="Create">
</form>

